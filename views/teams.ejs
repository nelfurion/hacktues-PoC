<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title>HackTUES - Отбори</title>
        <style media="screen">
            table {
                border-collapse: collapse;
            }

            table, th, td {
                border: 1px solid black;
            }
        </style>
    </head>
    <body>
        <table>
            <tr>
                <th>Отбор</th>
                <th>Проект</th>
                <th>Технологии</th>
            </tr>
            <% for (var i = 0; i < teams.length; i++) { %>
                <tr>
                    <td><%= teams[i].name %></td>
                    <td><%= teams[i].projectName %></td>
                    <td><%= teams[i].technologies %></td>
                </tr>
            <% } %>
        </table>
    </body>
</html>
